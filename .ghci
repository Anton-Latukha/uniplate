:set -w -fwarn-unused-binds -fwarn-unused-imports
:load Main

:def test       \x -> return $ ":main test"
:def build      \x -> return $ ":!ghc --make -O2 -hidir obj -odir obj Main.hs -o obj/main.exe"
:def exec       \x -> return $ ":build\n:!obj\\main.exe " ++ x
:def benchmark  \x -> return $ ":exec benchmark"
